.container{
	display: flex;
	flex-direction: column;
	align-items: center;
  background-color:#b3d4db;
}
.user-avatar{
	width:200rpx;
	height:200rpx;
	margin-top: 160rpx;
  border-radius: 50%;
}
.user-name{
	margin-top: 100rpx;
	font-size: 32rpx;
	font-weight: bold;
}

.moto{
	font-size: 26rpx;
	font-weight: bold;
	line-height: 80rpx;
	color: #405f80;
}
.moto-container{
	margin-top: 200rpx;
	border: 1px solid #405f80;
	width:  200rpx;
	height: 80rpx;
	text-align: center;
	border-radius: 10rpx;
}
page{
	height: 100%;
	background-color: #b3d4db;
}
